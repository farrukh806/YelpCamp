<%- include('./partials/header'); -%>

   
<form class="form-group text-center" action="../register" method="POST">
    <h1>Sign Up</h1>
    <input class="form-group text-center" type="text" name="username" placeholder="Username" autocomplete="off" required>
    <br>
    <input class="form-group text-center" type="password" name="password" placeholder="Password" autocomplete="off" required>
    <br>
    <button class="btn btn-primary" type="submit">Submit</button>
</form>

